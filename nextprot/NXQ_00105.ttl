prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00105
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with at least one cross-reference to SMR (Swiss Model Repository) but no cross-references to PDB''' ;
  sh:select '''select distinct ?entry where {
 ?entry a np:Entry.
 ?entry np:reference / np:provenance db:SMR.
 filter not exists { ?entry np:reference / np:provenance db:PDB }
}'''
.
