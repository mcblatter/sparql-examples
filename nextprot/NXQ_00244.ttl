prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00244
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with a variant having an impact on a binary interaction ''' ;
  sh:select '''select distinct ?entry ?interactant where {
 ?entry np:isoform / np:proteoform / np:phenotypicVariation ?phvar .
 ?phvar np:term / np:childOf nextprot_cv:ME_0000002 . # children of impact
 ?phvar np:evidence / np:quality np:GOLD .
 ?phvar np:impactedObject / np:interactant ?interactant.
}'''
.
