prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00015
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with a PDZ domain that interact with at least one protein which is expressed in brain''' ;
  sh:select '''select distinct ?entry where {
 ?entry np:isoform ?iso.
 ?iso np:region / np:term nextprot_cv:DO-00477. #PDZ domain
 ?iso np:binaryInteraction/np:interactant/np:isoform?/np:detectedExpression/np:term/np:childOf nextprot_cv:TS-0095 #brain
}'''
.
