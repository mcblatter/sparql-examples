prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00021
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with at least one HPA antibody that are located in the peroxisome''' ;
  sh:select '''select distinct ?entry where {
 values ?peroxiloc {nextprot_cv:GO_0005777 nextprot_cv:SL-0204} # GO and SL values for peroxisome
 
 ?entry np:isoform ?iso. 
 ?iso np:antibodyMapping ?map. 
 ?iso np:cellularComponent ?loc .
 ?loc np:term /np:childOf ?peroxiloc. # peroxisomal
 filter not exists { ?loc np:negativeEvidence ?negev. } # No negative localization evidence
}'''
.
