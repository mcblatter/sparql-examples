prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00009
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with 3 disulfide bonds and that are not annotated as hormones ''' ;
  sh:select '''select distinct ?entry where {
 ?entry np:isoform ?isoform.
 ?isoform np:ptm ?statement.
 ?statement a np:DisulfideBond.
 filter not exists { ?entry np:isoform / np:function / np:term /np:childOf nextprot_cv:GO_0005179. } # GO Hormone activity
} group by ?entry ?isoform having (count(?statement) =3 )'''
.
