prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00107
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with a protein existence not \"At protein level\" (PE=1) and with a HGNC approved gene name that contains \"orf\"''' ;
  sh:select '''select distinct ?entry where {
 ?entry np:existence ?level.
 filter (?level != np:Evidence_at_protein_level).
 ?entry np:reference ?r.
 ?r np:provenance db:HGNC ; np:accession ?ac.
 filter (regex(?ac,'orf')) .
}'''
.
