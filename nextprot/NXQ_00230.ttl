prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00230
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Proteins with experimentally determined lengthy alpha-helices (> 75 aa)''' ;
  sh:select '''select distinct ?entry (str(?gen) as ?gene) ?hlen where {
 ?entry np:isoform ?iso.
 ?entry np:gene / np:recommendedName / rdfs:label ?gen.
 ?iso np:helix ?hel .
 ?hel np:start ?s; np:end ?e .
 bind((?e - ?s + 1) as ?hlen )
 filter(?hlen > 75)
} 
order by desc(?hlen)'''
.
