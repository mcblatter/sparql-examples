prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00295
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''References for SCN1A variants causing Dravet syndrome''' ;
  sh:select '''select distinct (str(?publiid) as ?PMID) ?title where {
 entry:NX_P35498 np:isoform ?iso. #SCN1A entry
 ?iso np:swissprotDisplayed true. #Swissprot canonical isoform 
 {
 ?iso np:variant ?v.
 ?v np:disease nextprot_cv:DI-01023. #UniProtKB term for Dravet syndrome
 ?v np:evidence /np:reference ?pub.
 ?pub np:title ?title.
 ?pub np:from ?xref .
 ?xref np:accession ?publiid ; np:provenance db:PubMed .
 }
 union
 {
 ?iso np:proteoform ?pf.
 ?pf np:diseaseRelatedVariant ?ann.
 filter not exists {?ann np:negativeEvidence ?negev} # No negative disease evidence
 ?ann np:impactedObject /np:term nextprot_cv:C116573. #NCI Thesaurus term for Dravet syndrome
 ?ann np:evidence /np:reference ?pub.
 ?pub np:title ?title.
 ?pub np:from ?xref .
 ?xref np:accession ?publiid ; np:provenance db:PubMed .
 }
}'''
.
