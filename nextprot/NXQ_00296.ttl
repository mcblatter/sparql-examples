prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
ex:NXQ_00296
 sh:prefixes _:sparql_examples_prefixes ;
  rdfs:comment '''Pathways in which SCN1A GOLD interactants are involved''' ;
  sh:select '''select distinct ?pathway where {
 entry:NX_P35498 np:isoform /np:binaryInteraction ?interaction.
 ?interaction np:interactant ?entry; np:quality np:GOLD.
 ?entry a np:Entry.
 ?entry np:isoform /np:pathway /rdfs:comment ?pathway.
}
order by asc(?pathway)'''
.
