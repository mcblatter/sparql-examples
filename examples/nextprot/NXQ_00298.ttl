prefix ex: <https://sparql.nextprot.org/.well-known/sparql-examples>
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>
prefix schema:<http://schema.org/>
ex:NXQ_00298
  sh:prefixes _:sparql_examples_prefixes, _:nextprot_sparql_examples_prefixes ;
  rdfs:comment '''Recommended isoform names for MSH6''' ;
 schema:keyword "isoforms";
 schema:keyword "tutorial";
  sh:select '''select distinct ?iso ?name where {
 nextprot:NX_P52701 :isoform ?iso. # MSH6 entry
 ?iso :recommendedName ?name_entity .
 ?name_entity a :IsoformName; rdfs:label ?name.
}
order by asc(?iso)'''
.
